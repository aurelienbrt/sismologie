<!DOCTYPE html>
<html lang="fr">
  <head>
    <meta charset="utf-8">
    <link rel="stylesheet" href="stylesheets/style.css">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link href="https://fonts.googleapis.com/css?family=Montserrat" rel="stylesheet">
    <title>Sismologie</title>
  </head>
  <body>

<!-- HEADER WESBITE -->
<div class="parent">

  <div class="enfant-graph">
    <img src="images/photo.png" width="100%" height="190vh" alt="">
    <!--<canvas id="canvas"></canvas>-->
  </div>

  <div class="enfant-menu">
    <ul>
        <li class="active">Travaux Pratiques</li>
        <a href="/"><li>Sciences Participatives</li></a>
    </ul>
  </div>

</div>


<!-- PARENT BODY -->
<div class="parent-body">

    <div class="enfant-left">

        <h3>Fichiers SG2</h3>

        <ul>
          <% filesSG.forEach(function(file) { %>
            <a id="linkfile" href="download/sg2/<%= file %>" download>
            <li id="namefile"><%= file %></li>
            </a><br>
          <% }); %>
        </ul>

    </div>


    <div class="enfant-right">

        <h3>Fichiers Labview</h3>

        <ul>
          <% filesLVM.forEach(function(file) { %>
            <a id="linkfile" href="download/labview/<%= file %>" download>
            <li id="namefile"><%= file %> </li>
            </a><br>
          <% }); %>
        </ul>

    </div>

</div>


  </body>

</html>
